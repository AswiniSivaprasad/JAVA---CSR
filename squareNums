package Patterns;

import java.util.Scanner;

public class squareNums {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter n: ");
		int n = sc.nextInt();
		for(int i=1;i<=n;i++) {           //Loops for Rows
			for(int j=1;j<=n;j++) {       //Loop for number of stars in each row
				System.out.print(i);  //Printing n stars in a rows
			}
			System.out.println();         //Printing n starts in n rows
		}
	}
}
