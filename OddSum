package NumberManipulation;

import java.util.Scanner;

public class OddSum {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter n: ");
		int n = sc.nextInt();
		int Oddsum = 0;
		for(int i=1;i<=n;i++) {
			if(i%2!=0) {
				Oddsum = Oddsum+i;
			}
		}
        System.out.print("Sum of odd numbers is: "+Oddsum);
	}

}
