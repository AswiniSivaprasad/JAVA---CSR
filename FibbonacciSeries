package NumberManipulation;

import java.util.Scanner;

public class FibbonacciSeries {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter max number");
		int n = sc.nextInt();
		int fib;
		int n1 = 0;
		int n2 = 1;
		while(n1<=n) {
			if(n1>=0) {
				System.out.print(n1+" ");
			}
			fib = n1 + n2;
			n1 = n2;
			n2 = fib;
		}
	}
}
